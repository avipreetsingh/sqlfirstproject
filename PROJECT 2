CREATE TABLE Veegies(

veggie_id varchaer(20),
veggie_price money,
veggie_name varchar(20),
veggie_type varchar(20),
primary
);


CREATE TABLE lot(

lot_id varchaer(20),
quantity_of_vegetables int,
lot_name varchar(20),
veggie_id varchar(20)
primary key (lot_id)
);


SELECT TABLE_SCHEMA ,
       TABLE_NAME ,
       COLUMN_NAME ,
       ORDINAL_POSITION ,
       COLUMN_DEFAULT ,
       DATA_TYPE ,
       CHARACTER_MAXIMUM_LENGTH ,
       NUMERIC_PRECISION ,
       NUMERIC_PRECISION_RADIX ,
       NUMERIC_SCALE ,
       DATETIME_PRECISION
FROM   INFORMATION_SCHEMA.COLUMNS;

